{
  "framework": null,
  "buildCommand": "node vercel-build.js",
  "outputDirectory": "dist/public",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "NODE_ENV": "production",
    "SKIP_VITE_CONFIG_TS": "true"
  }
}
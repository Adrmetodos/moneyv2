{
  "framework": null,
  "buildCommand": "node vercel-build.js",
  "outputDirectory": "dist/public",
  "cleanUrls": true,
  "trailingSlash": false,
  "public": true,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html", "status": 200 }
  ],
  "github": {
    "silent": true
  }
}
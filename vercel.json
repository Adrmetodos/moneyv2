{
  "framework": null,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "STRIPE_SECRET_KEY": "${STRIPE_SECRET_KEY}",
    "VITE_STRIPE_PUBLIC_KEY": "${VITE_STRIPE_PUBLIC_KEY}",
    "SECRET_LIVE_API_KEY": "${SECRET_LIVE_API_KEY}"
  }
}
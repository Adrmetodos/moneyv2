{
  "framework": null,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "STRIPE_SECRET_KEY": "@stripe_secret_key",
    "VITE_STRIPE_PUBLIC_KEY": "@vite_stripe_public_key"
  }
}